{
  "name": "sval",
  "version": "0.4.8",
  "description": "A javascript interpreter written in javascript",
  "type": "module",
  "main": "dist/sval.cjs",
  "module": "dist/sval.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "prepare": "vite build && npm run post",
    "build": "vite build && npm run post",
    "start": "node scripts && vite build && npm run post",
    "post": "mv types/src/index.d.ts types && rm -rf types/src",
    "test": "node scripts && umi-test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siubaak/sval.git"
  },
  "keywords": [
    "sval",
    "js",
    "javascript",
    "eval",
    "interpreter"
  ],
  "author": "baakqiu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Siubaak/sval/issues"
  },
  "homepage": "https://github.com/Siubaak/sval#readme",
  "dependencies": {
    "acorn": "^8.8.1"
  },
  "devDependencies": {
    "@types/estree": "1.0.0",
    "@types/jest": "^29.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "umi-test": "^1.9.7",
    "vite": "^4.0.3"
  }
}
